@model IEnumerable<Personal.Models.PostShortDescription>

@{
    ViewBag.Title = ".Net Blog";
}

<h2>Blog</h2>


@foreach (var item in Model) {

    <div class="page-tab page-tab-text">@Html.DisplayFor(modelItem => item.Title)</div>
    
    <div class="row col-md-12 container">
            <p class="lead">
                @Html.DisplayFor(modelItem => item.Description)
            </p>
            <p> 
                @Html.Partial(item.PartialViewName)
            </p>
    </div>
}

